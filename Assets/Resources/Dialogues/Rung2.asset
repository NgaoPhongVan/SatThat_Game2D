  m_Script: {fileID: 11500000, guid: 09a157970672af643b0a768ab9316bb4, type: 3}
  m_Name: Rung2
  m_EditorClassIdentifier: 
  lines:
  - characterName: "Ng\u1ECDc"
    text: "Tr\xFAc L\xE2m, ng\u01B0\u1EDDi c\xF3 sao kh\xF4ng? B\u1ECDn ch\xFAng
      c\xF3 l\xE0m g\xEC ng\u01B0\u1EDDi kh\xF4ng?"
    characterSprite: {fileID: 21300000, guid: 21425693e22adb34a8ada297641bcb62, type: 3}
  - characterName: "Tr\xFAc L\xE2m"
    text: "(m\u1EC9m c\u01B0\u1EDDi y\u1EBFu \u1EDBt):\n\"Ta kh\xF4ng sao, nh\u01B0ng
      con kh\xF4ng n\xEAn m\u1EA1o hi\u1EC3m nh\u01B0 v\u1EADy. B\u1ECDn ch\xFAng
      kh\xF4ng d\u1EC5 \u0111\u1ED1i ph\xF3 \u0111\xE2u.\""
    characterSprite: {fileID: 21300000, guid: 3581c50598ac5cc46925b9b556444f75, type: 3}
  - characterName: "Ng\u1ECDc"
    text: "Gia \u0111\xECnh l\xE0 t\u1EA5t c\u1EA3. Con kh\xF4ng th\u1EC3 \u0111\u1EE9ng
      nh\xECn m\xE0 kh\xF4ng l\xE0m g\xEC."
    characterSprite: {fileID: 21300000, guid: 21425693e22adb34a8ada297641bcb62, type: 3}
  - characterName: "Tr\xFAc L\xE2m"
    text: "\u0110\u1EC3 c\u1EA3m \u01A1n, ta mu\u1ED1n trao cho ng\u01B0\u01A1i m\u1ED9t
      th\u1EE9 m\xE0 ta \u0111\xE3 gi\u1EEF r\u1EA5t l\xE2u."
    characterSprite: {fileID: 21300000, guid: 3581c50598ac5cc46925b9b556444f75, type: 3}
  - characterName: "Tr\xFAc L\xE2m"
    text: "\u0110\xE2y l\xE0 b\xED k\xEDp 'Th\u1EADp B\xE1t Ban V\xF5 Ngh\u1EC7',
      do t\u1ED5 ti\xEAn ch\xFAng ta \u0111\u1EC3 l\u1EA1i. \u0110\u1EBFn l\xFAc
      con ti\u1EBFp nh\u1EADn n\xF3 r\u1ED3i. H\xE3y d\xF9ng n\xF3 \u0111\u1EC3 b\u1EA3o
      v\u1EC7 nh\u1EEFng \u0111i\u1EC1u quan tr\u1ECDng."
    characterSprite: {fileID: 21300000, guid: 3581c50598ac5cc46925b9b556444f75, type: 3}
  - characterName: "Tr\xFAc L\xE2m"
    text: "H\xE3y luy\u1EC7n t\u1EADp v\u1EDBi con H\u1ED5 X\xE1m kh\u1ED5ng l\u1ED3
      trong r\u1EEBng."
    characterSprite: {fileID: 21300000, guid: 3581c50598ac5cc46925b9b556444f75, type: 3}
  - characterName: "Ng\u1ECDc"
    text: "Con s\u1EBD kh\xF4ng l\xE0m ng\u01B0\u1EDDi th\u1EA5t v\u1ECDng. \u0110\xE2y
      s\u1EBD l\xE0 \u0111\u1ED9ng l\u1EF1c \u0111\u1EC3 con tr\u1EDF n\xEAn m\u1EA1nh
      m\u1EBD h\u01A1n."
    characterSprite: {fileID: 21300000, guid: 21425693e22adb34a8ada297641bcb62, type: 3}
